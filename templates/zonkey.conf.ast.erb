;;; database parameters for zonkey.agi
[database]
dbhost=<%= db_host %>
dbname=<%= db_name %>
dbuser=<%= db_user_user %>
dbpasswd=<%= db_user_pass %>
dbport=3306

[settings]
;asterisk_sounds=/var/lib/asterisk/zonkey
alert_email=<%= ast_notification_email %>
external_codec=ulaw
ivr_pause=1
node=sip:<%= @ip =>:<%= @ast_port %>
redis=<% if @opensips_floating_ip != "127.0.0.1" %><%= @opensips_floating_ip %><% else %><%= @opensips_ip %>:6379
parking_timeout=120

[agi]
dial_param=R
[billing]
; only bill following prefixes
; not enable by default, need to update
;billing_prefixes=91,1,011,9011


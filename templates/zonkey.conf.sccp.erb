;;; database parameters for zonkey.agi
[database]
dbhost=<%= db_host %>
dbname=<%= db_name %>
dbuser=<%= db_user_user %>
dbpasswd=<%= db_user_pass %>
dbport=3306

[ami]
host=127.0.0.1
port=5038
user=ami_user
pass=ami_pass

[settings]
;asterisk_sounds=/var/lib/asterisk/zonkey
alert_email=<%= ast_notification_email %>
external_codec=ulaw

ivr_pause=1
; asterisk server for parking
;node=sip:10.130.12.31:5060
; redis address for parking
;redis=10.130.8.20:6379
; parking timeout for parking
;parking_timeout=30

[agi]
dial_param=
;behind_nat=yes

[billing]
enable=1
check_balance=0
; only bill following prefixes
billing_prefixes=91,1,011,9011
; before check in rate table, we need to remove the following prefixes, 9:1 means remove prefix 9 and prepend 1
remove_prefixes=9011,011,9:1




